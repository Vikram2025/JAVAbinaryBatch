package AssignmentsSolutionCode;

public class SumOfArray {
	public static int ArraySum(int[] numbers) {
		int sum=0;
		for(int num :numbers) {
			sum+=num;
		}
		return sum;
	}

	public static void main(String[] args) {
		
		int []numbers= {1,33,55,22,44};
		
		int SUM=ArraySum(numbers);
		System.out.println(SUM);
	}
}
