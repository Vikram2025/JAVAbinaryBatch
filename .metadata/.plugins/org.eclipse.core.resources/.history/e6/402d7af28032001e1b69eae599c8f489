package AssignmentsSolutionCode;

public class ArraySumExample {
public static int ArraySum(int[] arr) {
	int sum=0;
	for(int num :arr) {
		sum+=num;
		
	}
	return sum;
}
	public static void main(String[] args) {
		int[] arr= {10,20,30,40};
		int Total= ArraySum(arr);
		System.out.println(Total);
	}

}
